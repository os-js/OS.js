{
  "handler": "arduino",

  "client" : {
    "Version" : "2.0-alpha67-arduino-yun",
    "APIURI": "/cgi-bin/osjs-api",
    "FSURI":  "/cgi-bin/osjs-fs?path=",
    "System" : {
      "AutoStart": ["ServiceArduino"]
    },
    "VFS": {
      "Mountpoints": {
        "shared": {
          "enabled": false
        },
        "root": {
          "description": "Arduino Storage"
        }
      }
    }
  },

  "server" : {
    "debugmode": false,
    "tmpdir": "/tmp",
    "vfs": {
      "mounts": {
        "root": "/"
      }
    }
  }
}
